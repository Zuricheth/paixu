void BucketSort()
{
	int a[1001], i, j, t,n;
	for (i = 0; i <= 1000; i++)
	{
		a[i] = 0;
	}
	printf("请输入你要排序的数有多少:>");
	scanf("%d", &n);
	for (i = 0; i < n; i++)
	{
		scanf("%d", &t);
		a[t]++;
	}
	for (i = 0; i <= 1000; i++)
	{
		for (j = 1; j <= a[i]; j++)
		{
			printf("%d ", i);
		}
	}
	getchar();
}
//O(2*(m+n))
